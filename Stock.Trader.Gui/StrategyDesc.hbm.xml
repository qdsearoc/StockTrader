<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Stock.Common"
                   namespace="Stock.Account">
 
  <class name="StrategyDesc" table="T_STRATEGY">
    <id name="Id">
      <generator class="identity" />
    </id>
     
    <property name="Name" column="Name" type="String"/>
    <property name="Desc" column="Desc" type="String"/>
    <property name="Dll" column="Dll" type="String"/>
    <property name="Clazz" column="Clazz" type="String"/>
    <property name="Enabled" column="ENABLE" type="Boolean"/>
    <property name="Date" column="DATE" type="DateTime"/>
  </class>
 
</hibernate-mapping>